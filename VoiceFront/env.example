# API Configuration
NEXT_PUBLIC_API_URL=[본인의web백엔드(Springboot) 주소]
NEXT_PUBLIC_WS_URL=ws://localhost:8000/v1/stream?token=devtoken
NEXT_PUBLIC_API_TOKEN=devtoken

# AI Service Keys
NEXT_PUBLIC_OPENAI_API_KEY=[your_openai_key_here]
NEXT_PUBLIC_ANTHROPIC_API_KEY=[your_anthropic_key_here]
# http 환경일 시 wss:// 대신 ws:// 사용
NEXT_PUBLIC_WS_URL=wss://[본인의detection_model 주소(https://는 제외)]/ws
NEXT_PUBLIC_AI_SERVER_URL=[본인의 LLM_model의 주소]

# call websoket
# http 환경일 시 wss:// 대신 ws:// 사용
NEXT_PUBLIC_CALL_WS=wss://[본인의web백엔드(Springboot) 주소(https://는 제외)]/ws/voip

# App Configuration
NEXT_PUBLIC_APP_NAME=Voice Guard
NEXT_PUBLIC_APP_VERSION=0.1.0
# Development
NODE_ENV=development

# Social URL
# 각 소셜에서 클라이언트 아이디 발급과 리다이렉트 URL 2개씩 등록 필요
# 구글 콜백 URL
#   [본인의 프론드엔드 주소]/callback-google-login
#   [본인의 프론드엔드 주소]/callback-google-link
# 네이버 콜백 URL
#   [본인의 프론드엔드 주소]/callback-naver-login
#   [본인의 프론드엔드 주소]/callback-naver-link
# 카카오 콜백 URL
#   [본인의 프론드엔드 주소]/callback-kakao-login
#   [본인의 프론드엔드 주소]/callback-kakao-link
NEXT_PUBLIC_LOGIN_GOOGLE_URL=https://accounts.google.com/o/oauth2/v2/auth?client_id=[본인이 발급받은 구글로그인 클라이언드 ID]&redirect_uri=[본인의 프론드엔드 주소]/callback-google-login&response_type=token&scope=openid
NEXT_PUBLIC_LOGIN_NAVER_URL=https://nid.naver.com/oauth2.0/authorize?response_type=token&client_id=[본인이 발급받은 네이버로그인 클라이언드 ID]&redirect_uri=[본인의 프론드엔드 주소]/callback-naver-login&state=TEST
NEXT_PUBLIC_LOGIN_KAKAO_URL=https://kauth.kakao.com/oauth/authorize?client_id=[본인이 발급받은 카카오 로그인 클라이언드 ID]&redirect_uri=[본인의 프론드엔드 주소]/callback-kakao-login&response_type=code
NEXT_PUBLIC_LINK_GOOGLE_URL=https://accounts.google.com/o/oauth2/v2/auth?client_id=[본인이 발급받은 구글로그인 클라이언드 ID]&redirect_uri=[본인의 프론드엔드 주소]/callback-google-link&response_type=token&scope=openid
NEXT_PUBLIC_LINK_NAVER_URL=https://nid.naver.com/oauth2.0/authorize?response_type=token&client_id=[본인이 발급받은 네이버로그인 클라이언드 ID]&redirect_uri=[본인의 프론드엔드 주소]/callback-naver-link&state=TEST
NEXT_PUBLIC_LINK_KAKAO_URL=https://kauth.kakao.com/oauth/authorize?client_id=[본인이 발급받은 카카오 로그인 클라이언드 ID]&redirect_uri=[본인의 프론드엔드 주소]/callback-kakao-link&response_type=code
NEXT_PUBLIC_CALL_BACK=[본인의 프론드엔드 주소]/oauth-callback